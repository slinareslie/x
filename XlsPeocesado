@echo off
REM Cambiar al directorio donde est√°n las carpetas
cd /d "C:\ruta\a\tu\carpeta\principal"

REM Recorrer todos los archivos .xlsx de "excel nuevo"
for %%F in ("excel nuevo\*.xlsx") do (
    echo Moviendo %%F a excel base...
    move "%%F" "excel base\"

    echo Ejecutando script PowerShell...
    powershell -ExecutionPolicy Bypass -File "ruta\al\script.ps1"
)

echo Proceso completado.
pause
