YourGallery.Items = 
AddColumns(
    GroupBy(
        Filter(
            [@'DirectorioTransportistaSIGAD'],
            Dropdown1_1.Selected.Value = "Nombre" &&
            (
                StartsWith(EMPTRANSPINTER, TextSearchBox1_2.Text) ||
                EndsWith(EMPTRANSPINTER, TextSearchBox1_2.Text)
            )
        ),
        "CODIGO",
        "GroupTransportistas"
    ),
    "FirstEmpresa", First(GroupTransportistas)
)
