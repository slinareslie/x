SELECT 
    a.CEMPTI AS CODIGO, 
    a.DNOMBRE AS EMPTRANSPINTER, 
    a.DDIRECCION AS DIRECCION, 
    a.CESTADO AS ESTADO,
    a.CRESAUTORI, 
    a.FINICIO, 
    a.FFIN, 
    a.CPAIS AS PAIS, 
    a.CTIPODOC AS TIPODOC, 
    a.CNRODOC AS NRODOC, 
    b.REPRESENTATE, 
    b.FECINI_REP, 
    b.FECFIN_REP
FROM 
    EMPREDTI A, 
    REP_LEGAL B
WHERE 
    A.CEMPTI = B.COD_OPERADOR
    AND B.tip_operador = 'E'
    AND A.FFIN >= 20250501 
    AND A.FFIN <= 20250531
    AND B.FFIN >= 20250501
    AND A.CEMPTI IN ('0030')
    AND A.CESTADO = '01';


